{
  "name": "joker",
  "version": "0.0.0",
  "description": "Simple and powerful testing for command-line apps",
  "keywords": [
    "testing",
    "test",
    "cli",
    "command-line",
    "command-line apps",
    "command-line applications"
  ],
  "main": "./src",
  "homepage": "https://github.com/amilajack/joker",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "lint": "eslint .",
    "test": " jest --runInBand",
    "ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/amilajack/joker.git"
  },
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/assertion-error": "^1.1.0",
    "@types/clone": "^0.1.30",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-bliss": "^3.2.0",
    "flow": "^0.2.3",
    "jest": "^24.0.0",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "assertion-error": "~1.1.0",
    "clone": "^2.1.2",
    "shell-quote": "^1.6.1"
  },
  "eslintConfig": {
    "extends": "bliss"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./tests/setup.js"
    ]
  }
}
